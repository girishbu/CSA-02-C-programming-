#include <stdio.h>

int main() {
    int array1[10], array2[10], i;
    int *p1 = array1; 
    int *p2 = array2;

   
    printf("Input array1 elements: ");
    for(i = 0; i < 10; i++)
        scanf("%d", p1++);

    p1 = array1; 

    
    for(i = 0; i < 10; i++) {
        *p2++ = *p1++;
    }

    
    printf("\nArray1: ");
    for(i = 0; i < 10; i++)
        printf("%d ", array1[i]);

    printf("\nArray2: ");
    for(i = 0; i < 10; i++)
        printf("%d ", array2[i]);

    return 0;
}
