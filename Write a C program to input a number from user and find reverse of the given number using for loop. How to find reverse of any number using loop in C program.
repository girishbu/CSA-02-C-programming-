#include <stdio.h>

int main() {
    int number, reverse = 0;
    
    printf("Enter an integer: ");
    scanf("%d", &number);
    
    for (; number != 0; number /= 10) {
        reverse = reverse * 10 + (number % 10);
    }
    
    printf("Reverse of the number: %d", reverse);
    
    return 0;
}
