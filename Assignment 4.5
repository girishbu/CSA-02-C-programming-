#include <stdio.h>

struct Employee {
    int eno;
    char ename[50];
    float salary;
};

int main() {
    int n;
    printf("Enter the number of employees: ");
    scanf("%d", &n);
    
    struct Employee employees[n];
    
   
    for (int i = 0; i < n; i++) {
        printf("\nEnter details of employee %d:\n", i+1);
        printf("Employee Number: ");
        scanf("%d", &employees[i].eno);
        printf("Employee Name: ");
        scanf("%s", employees[i].ename);
        printf("Employee Salary: ");
        scanf("%f", &employees[i].salary);
    }
    
    
    struct Employee highest_salary_employee = employees[0];
    for (int i = 1; i < n; i++) {
        if (employees[i].salary > highest_salary_employee.salary) {
            highest_salary_employee = employees[i];
        }
    }
    
    
    printf("\nEmployee with highest salary:\n");
    printf("Employee Number: %d\n", highest_salary_employee.eno);
    printf("Employee Name: %s\n", highest_salary_employee.ename);
    printf("Employee Salary: %.2f\n", highest_salary_employee.salary);
    
    return 0;
}
